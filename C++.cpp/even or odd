#include <iostream>
 
using namespace std;
 
int main() {
 
int N;
cin>>N;

if (N==0) {
cout<<"NULL"<<endl;
}
else if ((N>0) && ((N%2)==0)) {
cout<<"PAR POSITIVO"<<endl;
}
else if ((N>0) && (N%2!=0)) { 
cout<<"IMPAR NEGATIVO"<<endl;
}
else if ((N<0) && (N%2==0)) {
cout<<"PAR NEGATIVO"<<endl;
}
else ((N<0) && (N%2!=0)) {
cout<<"ÍMPAR NEGATIVO"<<endl;
}
}
